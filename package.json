{
  "name": "element-fabric-editor",
  "description": "vue-fabric-editor的element版本",
  "author": "June",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .vue,.js,.ts,.jsx,.tsx src --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.3.0",
    "@commitlint/config-conventional": "~19.2.2",
    "@typescript-eslint/eslint-plugin": "~7.9.0",
    "@typescript-eslint/parser": "~7.9.0",
    "cross-env": "~7.0.3",
    "eslint": "~9.3.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-prettier": "~5.1.3",
    "eslint-plugin-vue": "~9.26.0",
    "git-cz": "^4.9.0",
    "husky": "~9.0.11",
    "lint-staged": "~15.2.2",
    "prettier": "~3.2.5",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "typescript": "^5.4.5",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.16.6",
    "vue-tsc": "~1.8.27"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@typescript-eslint"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=9.0.2"
  },
  "lint-staged": {
    "packages/**/.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
